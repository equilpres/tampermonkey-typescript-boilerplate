{
	"name": "tampermonkey-typescript-boilerplate",
	"description": "Boilerplate for Tampermonkey userscripts in TypeScript",
	"version": "0.0.0",
	"author": {
		"name": "Слава Соколов",
		"username": "equilpres",
		"url": "https://github.com/equilpres"
	},
	"type": "module",
	"imports": {
		"#*": "./src/*"
	},
	"engines": {
		"node": "^18.18"
	},
	"license": "MIT",
	"scripts": {
		"build": "pnpm lint && cross-env NODE_NO_WARNINGS=1 rollup --config",
