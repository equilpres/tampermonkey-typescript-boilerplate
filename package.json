		"prettier-plugin-tailwindcss": "^0.5.14",
		"rollup": "^4.16.4",
		"rollup-obfuscator": "^4.1.1",
		"rollup-plugin-license": "^3.3.1",
		"rollup-plugin-postcss": "^4.0.2",
		"rollup-plugin-svelte": "^7.2.0",
		"svelte": "^4.2.15",
		"svelte-check": "^3.6.9",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.3"
