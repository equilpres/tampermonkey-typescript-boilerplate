		"@rollup/plugin-typescript": "^11.1.6",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@types/common-tags": "^1.8.4",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "^20.14.7",
		"@types/tampermonkey": "^5.0.3",
		"autoprefixer": "^10.4.19",
		"common-tags": "^1.8.2",
