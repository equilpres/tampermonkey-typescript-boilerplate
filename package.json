		"rollup-plugin-license": "^3.4.1",
		"rollup-plugin-postcss": "^4.0.2",
		"rollup-plugin-svelte": "^7.2.2",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.1",
		"tailwindcss": "^3.4.4",
